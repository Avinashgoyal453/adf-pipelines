{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "avidatafac"
		},
		"AdvWorkDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorkDb'"
		},
		"BlobStorageForProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageForProducts'"
		},
		"CustomersBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CustomersBlobStorage'"
		},
		"CustomersDb1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CustomersDb1'"
		},
		"AdvWorkDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver1-avinash.database.windows.net"
		},
		"AdvWorkDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "avinashdb-1"
		},
		"AdvWorkDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Avinash"
		},
		"CustomersDb1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver1-avinash.database.windows.net"
		},
		"CustomersDb1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "avinashdb-1"
		},
		"CustomersDb1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Avinash"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "avidatafac_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorkDb_password": {
						"value": "[parameters('AdvWorkDb_password')]"
					},
					"BlobStorageForProducts_connectionString": {
						"value": "[parameters('BlobStorageForProducts_connectionString')]"
					},
					"CustomersBlobStorage_connectionString": {
						"value": "[parameters('CustomersBlobStorage_connectionString')]"
					},
					"CustomersDb1_password": {
						"value": "[parameters('CustomersDb1_password')]"
					},
					"AdvWorkDb_properties_typeProperties_server": {
						"value": "[parameters('AdvWorkDb_properties_typeProperties_server')]"
					},
					"AdvWorkDb_properties_typeProperties_database": {
						"value": "[parameters('AdvWorkDb_properties_typeProperties_database')]"
					},
					"AdvWorkDb_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorkDb_properties_typeProperties_userName')]"
					},
					"CustomersDb1_properties_typeProperties_server": {
						"value": "[parameters('CustomersDb1_properties_typeProperties_server')]"
					},
					"CustomersDb1_properties_typeProperties_database": {
						"value": "[parameters('CustomersDb1_properties_typeProperties_database')]"
					},
					"CustomersDb1_properties_typeProperties_userName": {
						"value": "[parameters('CustomersDb1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}